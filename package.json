    "nodemon": "^2.0.4",
    "qs": "^6.9.4",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "snakecase-keys": "^3.1.2",
    "swagger-ui-express": "^4.1.4",
    "tsoa": "^2.5.14",
    "utf8": "^3.0.0",
    "uuid": "^8.2.0",
    "winston": "^3.2.1",
    "winston-cloudwatch": "^2.3.2",
    "xlsx": "^0.16.2"
  }
}
